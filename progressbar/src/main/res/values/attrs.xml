<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PercentProgressBar">
        <attr name="barHeight" format="dimension" />
        <attr name="borderSize" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="percent" format="float" />
        <attr name="direction" format="enum">
            <enum name="LEFT_TO_RIGHT" value="0" />
            <enum name="RIGHT_TO_LEFT" value="1" />
        </attr>
        <attr name="textSize" format="dimension" />
        <attr name="progressColor" format="color" />
        <attr name="backgroundColor" format="color" />
        <attr name="remainMode" format="boolean" />
    </declare-styleable>
</resources>